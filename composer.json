{
    "name": "beberlei/github2jira",
    "require": {
        "silex/silex": "*",
        "zend/zf2": "*"
    },
    "autoload": {
        "psr-0": {
            "Zend": "vendor/zend/zf2/library"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "zend/zf2",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/zendframework/zf2/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/zendframework/zf2.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
